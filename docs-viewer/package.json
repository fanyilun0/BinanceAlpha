    "marked": "^12.0.0",
    "react-markdown": "^10.1.0",
    "vue": "^3.4.15",
    "vue-chartjs": "^5.3.3",
    "vue-echarts": "^8.0.1",
    "vue-router": "^4.6.3"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.0.3",
